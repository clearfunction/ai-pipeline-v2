    await vitePage.close()
    
    // Test Vue link  
    const [vuePage] = await Promise.all([
      context.waitForEvent('page'),
      page.click('a[href="https://vuejs.org/"]')
    ])
    await expect(vuePage).toHaveURL(/vuejs.org/)
    await vuePage.close()
  })
})
